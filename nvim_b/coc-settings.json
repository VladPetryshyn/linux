{
  "snippets.extends": {
    "javascript": ["javascript"],
    "javascriptreact": ["javascript", "javascriptreact"],
    "typescript": ["typescript"],
    "typescriptreact": ["typescript", "typescriptreact"]
  },
  "codeLens.enable": false,
  "codeLens.separator": "▸",
  "list.indicator": "❯",
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.willSaveHandlerTimeout": 1000,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "json",
    "graphql",
    "css",
    "scss",
    "sass"
  ],
  "coc.source.file.ignoreHidden": false,
  "eslint.format.enable": true,
  "eslint.packageManager": "yarn",
  "typescript.autoClosingTags": true,
  "javascript.autoClosingTags": true,
  "javascript.format.enabled": true,
  "typescript.format.enabled": true,
  "list.insertMappings": {
    "<C-x>": "action:split",
    "<C-v>": "action:vsplit"
  },
  "list.normalMappings": {
    "s": "action:split",
    "v": "action:vsplit"
  },
  "emmet.excludeLanguages": ["markdown", "typescript"],
  "diagnostic.errorSign": "✘ ",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "prettier.trailingComma": "all",
  "eslint.autoFixOnSave": true
}
