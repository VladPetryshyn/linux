{
  "snippets.extends": {
    "javascript": ["javascript"],
    "javascriptreact": ["javascript", "javascriptreact"],
    "typescript": ["typescript"],
    "typescriptreact": ["typescript", "typescriptreact"]
  },
  "codeLens.enable": true,
  "codeLens.separator": "▸",
  "list.indicator": "❯",
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.willSaveHandlerTimeout": 1000,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "json",
    "graphql"
  ],
  "coc.source.around.firstMatch": false,
  "coc.source.file.ignoreHidden": false,
  "eslint.format.enable": true,
  "eslint.packageManager": "yarn",
  "explorer.file.showHiddenFiles": true,
  "explorer.icon.enabledNerdfont": true,
  "explorer.keyMappingMode": "none",
  "explorer.icon.source": "vim-devicons",
  "explorer.git.icon.rootStatus.stashed": " ",
  "explorer.git.icon.rootStatus.ahead": "⇡ ",
  "explorer.git.icon.rootStatus.behind": "⇣ ",
  "explorer.git.icon.rootStatus.conflicted": " ",
  "explorer.git.icon.status.added": " ",
  "explorer.git.icon.rootStatus.added": " ",
  "explorer.git.icon.status.renamed": "➥ ",
  "explorer.width": 30,
  "explorer.icon.enableNerdfont": true,
  "typescript.autoClosingTags": true,
  "javascript.autoClosingTags": true,
  "explorer.previewAction.onHover": false,
  "explorer.keyMappings.global": {
    "<cr>": ["expandable?", "expand", "open"],
    "v": "open:vsplit",
    "d": "delete",
    "e": false,
    "o": ["expandable?", "expand", "open"],
    "u": ["collapse"],
    "a": "addFile",
    "f": "search",
    "F": "searchRecursive",
    "A": "addDirectory",
    "r": "rename",
    "E": "open:split",
    "t": "open:tab"
  },
  "explorer.quitOnOpen": true,
  "javascript.format.enabled": true,
  "typescript.format.enabled": true,
  "list.insertMappings": {
    "<C-x>": "action:split",
    "<C-v>": "action:vsplit"
  },
  "list.normalMappings": {
    "s": "action:split",
    "v": "action:vsplit"
  },
  "emmet.excludeLanguages": ["markdown", "typescript"],
  "diagnostic.errorSign": "✘ ",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "prettier.trailingComma": "all",
  "eslint.autoFixOnSave": true
}
